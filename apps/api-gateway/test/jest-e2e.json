{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@lib/providers/(.*)": "<rootDir>/../libs/providers/src/$1",
    "@lib/providers": "<rootDir>/../libs/providers/src",
    "@entities/entities/(.*)": "<rootDir>/../libs/entities/src/$1",
    "@entities/entities": "<rootDir>/../libs/entities/src",
    "@lib/shared/(.*)": "<rootDir>/../libs/shared/src/$1",
    "@lib/shared": "<rootDir>/../libs/shared/src"
  }
}